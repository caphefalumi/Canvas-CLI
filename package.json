{
  "name": "canvaslms-cli",
  "version": "1.6.1",
  "description": "A command line tool for interacting with Canvas LMS API",
  "keywords": [
    "canvas",
    "cli",
    "api",
    "instructure",
    "lms",
    "education",
    "canvas-lms"
  ],
  "homepage": "https://github.com/caphefalumi/Canvas-CLI#readme",
  "bugs": {
    "url": "https://github.com/caphefalumi/Canvas-CLI/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caphefalumi/Canvas-CLI.git"
  },
  "license": "MIT",
  "author": {
    "name": "Your Name",
    "email": "your.email@example.com"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "canvaslms-cli": "dist/src/index.js",
    "canvas": "dist/src/index.js"
  },
  "files": [
    "dist/",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "bun dist/src/index.js",
    "prepublishOnly": "npm run build",
    "postinstall": "echo \"Canvas CLI installed successfully! Run 'canvas config' to get started.\""
  },
  "dependencies": {
    "adm-zip": "^0.5.16",
    "axios": "^1.6.0",
    "chalk": "^5.4.1",
    "commander": "^11.1.0",
    "form-data": "^4.0.3"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.7",
    "@types/minimatch": "^6.0.0",
    "@types/node": "^24.10.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "preferGlobal": true,
  "os": [
    "win32",
    "darwin",
    "linux"
  ]
}
