{
  "name": "canvaslms-cli",
  "version": "1.7.2",
  "description": "A command line tool for interacting with Canvas LMS",
  "keywords": [
    "api",
    "canvas",
    "canvas-lms",
    "cli",
    "education",
    "instructure",
    "lms"
  ],
  "homepage": "https://github.com/caphefalumi/Canvas-CLI#readme",
  "bugs": {
    "url": "https://github.com/caphefalumi/Canvas-CLI/issues"
  },
  "license": "MIT",
  "author": {
    "name": "caphefalumi",
    "email": "dangduytoan13l@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caphefalumi/Canvas-CLI.git"
  },
  "bin": {
    "canvas": "dist/src/index.js",
    "canvaslms-cli": "dist/src/index.js"
  },
  "files": [
    "CHANGELOG.md",
    "README.md",
    "dist/"
  ],
  "os": [
    "win32",
    "darwin",
    "linux"
  ],
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "bun dist/src/index.js",
    "lint": "oxlint --fix --fix-suggestions",
    "format": "oxfmt --config .oxfmtrc.jsonc",
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "prepublishOnly": "bun run build && bun test",
    "prepare": "bunx husky"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "form-data": "^4.0.5"
  },
  "devDependencies": {
    "@types/bun": "^1.3.4",
    "@types/minimatch": "^6.0.0",
    "@types/node": "^25.0.2",
    "husky": "^9.1.7",
    "oxfmt": "^0.18.0",
    "oxlint": "^1.33.0",
    "typescript": "^5.9.3"
  },
  "preferGlobal": true,
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  }
}
