{
  "name": "canvaslms-cli",
  "version": "1.6.6",
  "description": "A command line tool for interacting with Canvas LMS API",
  "keywords": [
    "canvas",
    "cli",
    "api",
    "instructure",
    "lms",
    "education",
    "canvas-lms"
  ],
  "homepage": "https://github.com/caphefalumi/Canvas-CLI#readme",
  "bugs": {
    "url": "https://github.com/caphefalumi/Canvas-CLI/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caphefalumi/Canvas-CLI.git"
  },
  "license": "MIT",
  "author": {
    "name": "Your Name",
    "email": "your.email@example.com"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "canvaslms-cli": "dist/src/index.js",
    "canvas": "dist/src/index.js"
  },
  "files": [
    "dist/",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "bun dist/src/index.js",
    "lint": "oxlint --fix --fix-suggestions",
    "format": "oxfmt --config .oxfmtrc.jsonc",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "prepublishOnly": "npm run build",
    "prepare": "bunx husky"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "chalk": "^5.4.1",
    "commander": "^11.1.0",
    "form-data": "^4.0.3"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/minimatch": "^6.0.0",
    "@types/node": "^24.10.0",
    "husky": "^9.1.7",
    "oxfmt": "^0.17.0",
    "oxlint": "^1.32.0",
    "prettier": "^3.0.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "preferGlobal": true,
  "os": [
    "win32",
    "darwin",
    "linux"
  ]
}
